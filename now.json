{
    "version": 2,
    "name": "reshadow",
    "alias": "reshadow",
    "builds": [{
        "src": "package.json",
        "use": "@now/static-build"
    }],
    "routes": [{
        "src": "/(static|public)/(.*)",
        "dest": "/$1/$2"
    }, {
        "src": "^/(.*)",
        "dest": "/index.html"
    }]
}
